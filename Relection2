        "gpt_4o = ChatOpenAI(model='gpt-4o-mini',\n",
        "         api_key='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',\n",
        "         base_url='https://openrouter.ai/api/v1')\n"
      ],
