    {
      "cell_type": "code",
      "source": [
        "class QA(TypedDict):\n",
        "  question    : str\n",
        "  answer      : str\n",
        "  critique    : str\n",
        "  is_satisfy  : bool\n"
      ],
      "metadata": {
        "id": "P5gzYoVKbVig"
      },
      "execution_count": 91,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def generate_answer(State:QA):\n",
        "  prompt = f\"\"\"\n",
        "            Your job is to answer for the following question :\n",
        "            {QA[\"question\"]}\n",
        "            \"\"\"\n",
        "  answer = gpt_4o.invoke(prompt).content\n",
        "\n",
        "  return {\"answer\":answer}\n",
        "\n",
        "\n",
        "def critique_answer(State:QA):\n",
        "  prompt = f\"\"\"\n",
        "            Your are a strict reviewer :\n",
        "            question :  {State[\"question\"]}\n",
        "            answer :    {State[\"answer\"]}\n",
        "            CRITIQUE the  anwer\n",
        "            if it good respond your reply with\n",
        "            SATISFACTORY\n",
        "            if it is not good reply why it is\n",
        "            not good and suggest improvement as well\n",
        "            \"\"\"\n",
        "  critique =  gpt_4o.invoke(prompt).content\n",
        "\n",
        "  is_satisfy =  'SATISFACTORY' in critique\n",
        "\n",
        "  return {\"critique\":critique,\"is_satisfy\":is_satisfy}\n",
        "\n",
        "def rewrite_answer(State:QA):\n",
        "  prompt = f\"\"\"\n",
        "\n",
        "            Improve the following answer based on critique :\n",
        "            question   :   {State[\"question\"]}\n",
        "            answer     :   {State[\"answer\"]}\n",
        "            critique   :   {State[\"critique\"]}\n",
        "\n",
        "            Your job is to answer a better answer\n",
        "            \"\"\"\n",
        "  answer = gpt_4o.invoke(prompt).content\n",
        "\n",
        "  return {\"answer\":answer}\n",
        "\n"
      ],
      "metadata": {
        "id": "5rfhut9k0sKg"
      },
      "execution_count": 104,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def route(State:QA):\n",
        "  if State[\"is_satisfy\"]:\n",
        "    return \"__end__\"\n",
        "  return \"rewrite\"\n"
      ],
      "metadata": {
        "id": "pmroFDvv2uAA"
      },
      "execution_count": 93,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "graph1 = StateGraph(QA)\n",
        "\n",
        "graph1.add_node(\"generate\",generate_answer)\n",
        "graph1.add_node(\"critique\",critique_answer)\n",
        "graph1.add_node(\"rewrite\",rewrite_answer)\n",
        "\n",
        "graph1.add_edge(START,\"generate\")\n",
        "graph1.add_edge(\"generate\",\"critique\")\n",
        "graph1.add_conditional_edges(\"critique\",route,{\"__end__\":\"__end__\",\"rewrite\":\"rewrite\"})\n",
        "\n",
        "graph1.add_edge(\"rewrite\",\"critique\")\n",
        "\n",
        "builder1 = graph1.compile()\n"
      ],
